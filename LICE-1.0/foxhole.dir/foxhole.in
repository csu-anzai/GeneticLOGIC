# run                 = <not set> # Experiment Name
popsize              =        600 # Population Size
vars                 =          2 # No Of Variables
sigmas               =          1 # No Of Sigmas
select               =      comma # Selection Scheme
reco_x               =   discrete # Recombination x[i]
reco_s               = arithmetic # Recombination sigma[i]
children             =          5 # Children Per Individual
nbhsize              =          1 # Neighborhood Size
tau0                 =        0.1 # Global Sigma Variance
taui                 =        0.3 # Local Sigma Variance
min_init_x           =        500 # Lower Boundary For Initial x[i]
max_init_x           =        500 # Upper Boundary For Initial x[i]
min_init_s           =       1e-4 # Lower Boundary For Initial sigma[i]
max_init_s           =        1e4 # Upper Boundary For Initial sigma[i]
smin                 =      1e-50 # Minimum sigma[i]
smax                 =       1e50 # Maximum sigma[i]
# stopat              = <not set> # Stop when f(x)	is better
maxgen               =        300 # Maximum No Of Generations
# maxcalls            = <not set> # Maximum No Of Function Calls
dmpint               =          0 # Dump Interval
# seed                = <not set> # Random Seed
# logfile             = <not set> # Logfile
# flushlog            = <not set> # Flush Interval
pixmon               =        yes # Use PixMon
showxs               =         no # Show best's x and sigma
showy                =         no # Show best's f(x)
